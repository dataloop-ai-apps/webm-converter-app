{
  "webm-converter": {
    "*": {
      "webm_upload_path": "/.dataloop/webm",
      "conversion_method": "ffmpeg"
    },
    "dataset-id-1": {
      "webm_upload_path": "/.dataloop/webm/dataset-id-1"
    },
    "dataset-id-2": {
      "webm_upload_path": "/.dataloop/webm/dataset-id-2"
    },
    "dataset-id-3": {
      "webm_upload_path": "/.dataloop/webm/dataset-id-3"
    }
  },
  "video-thumbnail": {
    "*": {
      "thumbnail_upload_path": "/.dataloop/thumbnails",
      "width": 256,
      "height": 144,
      "aspect_ratio": "16:9",
      "n_frames_sampling": 10
    },
    "dataset-id-1": {
      "thumbnail_upload_path": "/.dataloop/thumbnails/dataset-id-1",
      "width": 100,
      "height": 100
    }
  },
  "video-metadata-extractor": {
    "*": {
      "extract_audio_metadata": true,
      "count_frames": true
    }
  }
